{
	"name": "moxio/captainhook-eslint",
	"type": "library",
	"description": "Captain Hook Plugin to validate files using ESLint",
	"license": "MIT",
	"keywords" : [ "captainhook", "eslint", "git", "pre-commit" ],
	"authors": [
		{
			"name": "Moxio",
			"homepage": "https://www.moxio.com"
		}
	],
	"autoload": {
		"psr-4": {
			"Moxio\\CaptainHook\\ESLint\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Moxio\\CaptainHook\\ESLint\\Test\\": "tests/"
		}
	},
	"require": {
		"php": "^7.2 || ^8.0",
		"captainhook/captainhook": "^4.4 || ^5.0",
		"sebastianfeldmann/cli": "^3.0",
		"sebastianfeldmann/git": "^2.2 || ^3.0"
	},
	"require-dev": {
		"phpunit/phpunit": "^8.5"
	}
}
